# Copie este arquivo para .env e preencha com seus dados

# URL base da API (local ou producao)
BASE_URL=http://localhost:3000

# URL do projeto Supabase (ex: https://xxxxxxxxxxx.supabase.co)
SUPABASE_URL=

# Anon/Public Key do Supabase (token JWT longo que comeca com eyJ...)
SUPABASE_ANON_KEY=

# Service Role Key do Supabase (NUNCA EXPOR NO FRONTEND!)
SUPABASE_SERVICE_ROLE_KEY=

# JWT Secret do Supabase (Settings > API > JWT Settings)
SUPABASE_JWT_SECRET=

# Chave da API4AI para reconhecimento de vinhos
API4AI_KEY=

# Porta do servidor
PORT=3000

# Ambiente (development ou production)
NODE_ENV=development
